n        .logo h1 {\n            font-size: 22px;\n            font-weight: 600;\n        }\n        \n        nav ul {\n            display: flex;\n            list-style: none;\n            gap: 25px;\n        }\n        \n        nav a {\n            color: #e0e0e0;\n            text-decoration: none;\n            font-weight: 500;\n            transition: color 0.3s;\n            position: relative;\n            padding: 5px 0;\n        }\n        \n        nav a:hover {\n            color: white;\n        }\n        \n        nav a::after {\n            content: '';\n            position: absolute;\n            bottom: 0;\n            left: 0;\n            width: 0;\n            height: 2px;\n            background: #4caf50;\n            transition: width 0.3s;\n        }\n        \n        nav a:hover::after {\n            width: 100%;\n        }\n        \n        .header-actions {\n            display: flex;\n            align-items: center;\n            gap: 15px;\n        }\n        \n        .search-box {\n            display: flex;\n            background: rgba(255, 255, 255, 0.1);\n            border-radius: 20px;\n            padding: 5px 15px;\n            align-items: center;\n        }\n        \n        .search-box input {\n            background: transparent;\n            border: none;\n            color: white;\n            padding: 8px;\n            width: 180px;\n            outline: none;\n        }\n        \n        .search-box input::placeholder {\n            color: #b0b0b0;\n        }\n        \n        .search-box button {\n            background: transparent;\n            border: none;\n            color: #b0b0b0;\n            cursor: pointer;\n            transition: color 0.3s;\n        }\n        \n        .search-box button:hover {\n            color: white;\n        }\n        \n        .auth-buttons {\n            display: flex;\n            gap: 10px;\n        }\n        \n        .btn {\n            padding: 8px 18px;\n         